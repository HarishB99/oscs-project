{
  "firewallRules": {
    "incoming": [
      {
        "priority": 100,
        "name": "http",
        "allow": true,
        "sourceip" : "*.*.*.*",
        "sourceport": "*",
        "destip": "*.*.*.*",
        "destport": "80",
        "protocol" : "tcp",
        "state" : "NEW,ESTABLISHED,RELATED"
      },
      {
        "priority": 200,
        "name": "ssh",
        "allow": true,
        "sourceip" : "*.*.*.*",
        "sourceport": "*",
        "destip": "*.*.*.*",
        "destport": "22",
        "protocol" : "tcp",
        "state" : "NEW,ESTABLISHED,RELATED"
      },
      {
        "priority": 1000,
        "name": "denyAll",
        "allow": false,
        "sourceip" : "*.*.*.*",
        "sourceport": "*",
        "destip": "*.*.*.*",
        "destport": "*",
        "protocol" : "*",
        "state" : "NEW,ESTABLISHED,RELATED"
      }
    ],
    "outgoing": [
      {
        "priority": 1000,
        "name": "allowAll",
        "allow": true,
        "sourceip" : "*.*.*.*",
        "sourceport": "*",
        "destip": "*.*.*.*",
        "destport": "*",
        "protocol" : "*",
        "state" : "NEW,ESTABLISHED,RELATED"
      }
    ],
    "webfilter" : {
      "global" : {
        "mode": "blacklist",
        "domainGroups" : ["socialMedia"],
        "domains": [
          "www.deviantart.com"
        ]
      }
    }
  },
  "dpi" : true,
  "virusScan" : true
}
