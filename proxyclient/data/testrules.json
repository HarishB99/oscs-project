{
  "firewallRules": {
    "rules":[
      {
        "priority": 100,
        "name": "http",
        "allow": true,
        "sourceip" : "0.0.0.0",
        "sourceport": "*",
        "destip": "0.0.0.0",
        "destport": "80",
        "protocol" : "tcp",
        "state" : "NEW,ESTABLISHED,RELATED",
        "direction" : "incoming"
      },
      {
        "priority": 200,
        "name": "ssh",
        "allow": true,
        "sourceip" : "0.0.0.0",
        "sourceport": "*",
        "destip": "0.0.0.0",
        "destport": "22",
        "protocol" : "tcp",
        "state" : "NEW,ESTABLISHED,RELATED",
        "direction": "incoming"
      },
      {
        "priority": 300,
        "name": "proxyserver",
        "allow": true,
        "sourceip": "0.0.0.0",
        "sourceport" : "*",
        "destip": "0.0.0.0",
        "destport": "8080",
        "protocol": "tcp",
        "state" : "NEW,ESTABLISHED,RELATED",
        "direction" : "incoming"
      },
      {
        "priority": 1000,
        "name": "denyAll",
        "allow": false,
        "sourceip" : "0.0.0.0",
        "sourceport": "*",
        "destip": "0.0.0.0",
        "destport": "*",
        "protocol" : "*",
        "state" : "NEW,ESTABLISHED,RELATED",
        "direction" : "incoming"
      },
      {
        "priority": 1000,
        "name": "allowAll",
        "allow": true,
        "sourceip" : "0.0.0.0",
        "sourceport": "*",
        "destip": "0.0.0.0",
        "destport": "*",
        "protocol" : "*",
        "state" : "NEW,ESTABLISHED,RELATED",
        "direction" : "outgoing"
      }
    ]
  },
  "webfilter" : {
    "mode": "blacklist",
    "blockAds": true,
    "blockMalicious": true,
    "domainGroups" : ["socialMedia"],
    "domains": [
      "kongregate.com"
    ]
  },
  "dpi" : true,
  "virusScan" : true
}
